<a href="#" class="delete" {{action "delete" on="click"}}>
<img src="/assets/delete-x.png"/></a>

{{#unless empty}}
	<img {{bindAttr src="photo.file.thumb.url"}}/>
{{else}}
	<a class="upload" {{action "selectPhoto" on="click"}}></a>
	<input class="photo-file" name="photo[file]" type="file" accept="image/*" {{action "uploadPhoto" on="change"}} />
{{/unless}}

<input type="hidden" name="order" {{bindAttr value="video.order"}}>
